					{
						"key": "request-app-id",
						"value": "{{request-app-id}}",
						"type": "text"
					},
					{
						"key": "customer-request-dt",
						"value": "{{customer-request-dt}}",
						"type": "text"
					},
					{
						"key": "entity-request-dt",
						"value": "{{entity-request-dt}}",
						"type": "text"
					},
					{
